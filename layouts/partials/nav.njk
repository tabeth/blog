<header class="sans-serif bg-black w-100 ph3 pv3 pv4-ns ph4-m ph5-l">
  <nav class="f6 fw6 ttu tracked">
    <div class="dib-ns db">
      <a class="link dim white dib mr3" href="/" title="home">tabeth</a>
      <div class="h1 bg-white"></div>
    </div>
    {% for collection in collections | keys %}
      {% if collection !== 'posts' %}
        <div class="dib-ns db">
          <a class="link dim white dib mr3" href="/{{collection}}" title="{{collection}}">{{collection}}</a>
          <div class="h1" style="background-color: {{ colors | get(collection) }}"></div>
        </div>
      {% endif %}
    {% endfor %}
    <div class="dib-ns db">
      <a class="link dim white dib mr3" href="/about" title="about">about</a>
      <div class="h1 bg-white"></div>
    </div>
  </nav>
</header>
